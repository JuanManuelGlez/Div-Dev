<%- include('../includes/head.ejs') %>

    <h1>Este es asignar ticket</h1>
    <div class="container">
        <div class="row justify-content-center">

                <form id="asignar_ticket" method="POST">
                  <input type="hidden" name="_csrf"id="_csrf" value="<%= csrfToken %>">
                  <select class="form-select" id="elegir_usuario" name="elegir_usuario" aria-label="Default select example">
                    <option selected value="<%=encargado[0].Id_Usuario%>"><%=encargado[0].Nombre_Usuario%><%=encargado[0].Tickets_Activos%></option> //ver como agragar la imagen
                    //Tickets_Activos puede cambiar!!
                    <% for (let usuario of usuarios){%>
                      
                        <option value="<%=usuario.Id_Usuario%>"><%=usuario.Nombre_Usuario%><%=usuario.Tickets_Activos%></option>
                        
                    <%}%>
                  </select><br>
                  <button type="submit" class="btn btn-primary mt-3" id="enviar">Submit</button>

                  </form>
        </div>
    </div>


    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta2/dist/js/bootstrap-select.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
