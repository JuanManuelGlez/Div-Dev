<%- include('../includes/head.ejs') %>
<div class="container" id="Completo">
    <h2>Completo:</h2>
    <%for (let ticket of ticket_completo){%>
        <div>
             ID: <%=ticket.Id_Ticket%>
             Asunto: <%=ticket.Asunto%>
             Prioridad: <%=ticket.Id_Prioridad%>
             Tipo de Incidencia: <%=ticket.Id_Tipo_Incidencia%>
        </div>
    <%}%>
</div> <br>

<div class="container" id="Progreso">
    <h2>PROGRESO:</h2>
    <%for (let ticket of ticket_progreso){%>
        <div>
             ID: <%=ticket.Id_Ticket%>
             Asunto: <%=ticket.Asunto%>
             Prioridad: <%=ticket.Id_Prioridad%>
             Tipo de Incidencia: <%=ticket.Id_Tipo_Incidencia%>
        </div>
    <%}%>
</div>


